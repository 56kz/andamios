<div class="container">
  <div class="row">
    <div class="col-sm-12">
      <h1 class="text-center">Sectores</h1><br><br>

      <table class="table">
        <tr>
          <th>Id</th>
          <th>Nombre</th>
        </tr>
        <% @sectors.each do |s| %>
          <tr>
            <td><%= s.id %></td>
            <td><%= s.name %></td>
          </tr>
        <% end %>
      </table>

    </div>
  </div>
</div>
